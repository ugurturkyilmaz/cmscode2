<html>
<head>
<title>CalendarEditCalendar</title>
</head>

<body>
<table border="1" cellpadding="1" cellspacing="1">
<thead>
<tr><td rowspan="1" colspan="3">CalendarEditCalendar</td></tr>
</thead>

<tbody>
<tr>
	<td>ENABLE_COMMENTS_CHECKBOX</td>
	<td>//input[contains(@id,'_enableComments')]</td>
	<td></td>
</tr>
<tr>
	<td>CHOOSE_MY_CALENDAR_COLOR</td>
	<td>//div[contains(@class,'simple-color-picker-item') and contains(@style,'${key_myCalendarColor}')]</td>
	<td></td>
</tr>
<tr>
	<td></td>
	<td></td>
	<td></td>
</tr>
<!--NAVIGATION-->
<tr>
	<td>NAVIGATION_NOTIFICATION_TEMPLATES_TAB</td>
	<td>//ul[contains(@class,'nav navbar-nav')]/li/a/span[contains(.,'Notification Templates')]</td>
	<td></td>
</tr>
<tr>
	<td></td>
	<td></td>
	<td></td>
</tr>
<!--NOTIFICATION_TEMPLATES-->
<tr>
	<td>NOTIFICATION_TEMPLATES_HEADER_TITLE</td>
	<td>//h3[@class='header-title']/span[contains(.,'${key_calendarName}')]</td>
	<td></td>
</tr>
<tr>
	<td>NOTIFICATION_TEMPLATES_INVITE_EMAIL_TAB_ACTIVE</td>
	<td>//ul[contains(@class,'nav navbar-nav')]//a[contains(@class,'active')]//span[contains(.,'Invite Email')]</td>
	<td></td>
</tr>
<tr>
	<td>NOTIFICATION_TEMPLATES_INVITE_EMAIL_TAB_INACTIVE</td>
	<td>//ul[contains(@class,'nav navbar-nav')]//a[not(contains(@class,'active'))]//span[contains(.,'Invite Email')]</td>
	<td></td>
</tr>
<tr>
	<td>NOTIFICATION_TEMPLATES_REMINDER_EMAIL_TAB_ACTIVE</td>
	<td>//ul[contains(@class,'nav navbar-nav')]//a[contains(@class,'active')]//span[contains(.,'Reminder Email')]</td>
	<td></td>
</tr>
<tr>
	<td>NOTIFICATION_TEMPLATES_REMINDER_EMAIL_TAB_INACTIVE</td>
	<td>//ul[contains(@class,'nav navbar-nav')]//a[not(contains(@class,'active'))]//span[contains(.,'Reminder Email')]</td>
	<td></td>
</tr>
<tr>
	<td>NOTIFICATION_TEMPLATES_FROM_NAME_FIELD</td>
	<td>//input[contains(@id,'_fromName')]</td>
	<td></td>
</tr>
<tr>
	<td>NOTIFICATION_TEMPLATES_FROM_ADDRESS_FIELD</td>
	<td>//input[contains(@id,'_fromAddress')]</td>
	<td></td>
</tr>
<tr>
	<td>NOTIFICATION_TEMPLATES_SUBJECT_FIELD</td>
	<td>//input[contains(@id,'_subject')]</td>
	<td></td>
</tr>
</tbody>
</table>
</body>
</html>