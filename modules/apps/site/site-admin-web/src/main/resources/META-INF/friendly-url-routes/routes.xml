<?xml version="1.0"?>
<!DOCTYPE routes PUBLIC "-//Liferay//DTD Friendly URL Routes 7.4.0//EN" "http://www.liferay.com/dtd/liferay-friendly-url-routes_7_4_0.dtd">

<routes>
	<route>
		<pattern>/site/{groupId:\d+}/edit</pattern>
		<implicit-parameter name="mvcPath">/edit_site.jsp</implicit-parameter>
		<implicit-parameter name="p_p_state">maximized</implicit-parameter>
	</route>
	<route>
		<pattern>/site/{parentGroupId:\d+}/new/{layoutSetPrototypeId:\d+}</pattern>
		<implicit-parameter name="mvcPath">/edit_site.jsp</implicit-parameter>
		<implicit-parameter name="p_p_state">maximized</implicit-parameter>
	</route>
	<route>
		<pattern>/sites/new/{layoutSetPrototypeId:\d+}</pattern>
		<implicit-parameter name="mvcPath">/edit_site.jsp</implicit-parameter>
		<implicit-parameter name="p_p_state">maximized</implicit-parameter>
	</route>
	<route>
		<pattern>/site/{parentGroupId:\d+}/new</pattern>
		<implicit-parameter name="mvcPath">/edit_site.jsp</implicit-parameter>
		<implicit-parameter name="p_p_state">maximized</implicit-parameter>
	</route>
	<route>
		<pattern>/sites/new/{layoutSetPrototypeId:\d+}</pattern>
		<implicit-parameter name="mvcPath">/edit_site.jsp</implicit-parameter>
		<implicit-parameter name="p_p_state">maximized</implicit-parameter>
	</route>
	<route>
		<pattern>/sites/new</pattern>
		<implicit-parameter name="mvcPath">/edit_site.jsp</implicit-parameter>
		<implicit-parameter name="p_p_state">maximized</implicit-parameter>
	</route>
	<route>
		<pattern>/sites/{parentGroupId:\d+}/select_site</pattern>
		<implicit-parameter name="mvcRenderCommandName">/site_admin/select_site_initializer</implicit-parameter>
		<implicit-parameter name="p_p_state">maximized</implicit-parameter>
	</route>
	<route>
		<pattern>/sites/select_site</pattern>
		<implicit-parameter name="mvcRenderCommandName">/site_admin/select_site_initializer</implicit-parameter>
		<implicit-parameter name="p_p_state">maximized</implicit-parameter>
	</route>
	<route>
		<pattern>/site/{groupId:\d+}</pattern>
		<implicit-parameter name="mvcPath">/view.jsp</implicit-parameter>
		<implicit-parameter name="p_p_state">maximized</implicit-parameter>
	</route>
	<route>
		<pattern>/sites</pattern>
		<ignored-parameter name="mvcPath" />
		<implicit-parameter name="p_p_state">maximized</implicit-parameter>
	</route>
	<route>
		<pattern></pattern>
		<ignored-parameter name="mvcPath" />
	</route>
</routes>