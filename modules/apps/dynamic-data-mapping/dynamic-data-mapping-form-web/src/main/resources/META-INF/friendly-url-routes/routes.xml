<?xml version="1.0"?>
<!DOCTYPE routes PUBLIC "-//Liferay//DTD Friendly URL Routes 7.4.0//EN" "http://www.liferay.com/dtd/liferay-friendly-url-routes_7_4_0.dtd">

<routes>
	<route>
		<pattern>/{formInstanceId:\d+}</pattern>
		<implicit-parameter name="p_p_id">com_liferay_dynamic_data_mapping_form_web_portlet_DDMFormPortlet</implicit-parameter>
		<implicit-parameter name="shared">true</implicit-parameter>
	</route>
	<route>
		<pattern>/{formInstanceId:\d+}/preview</pattern>
		<implicit-parameter name="p_p_id">com_liferay_dynamic_data_mapping_form_web_portlet_DDMFormPortlet</implicit-parameter>
		<implicit-parameter name="preview">true</implicit-parameter>
	</route>
</routes>