Bundle-Name: Liferay TLD Formatter
Bundle-SymbolicName: com.liferay.tld.formatter
Bundle-Version: 1.0.6
Import-Package:\
	!org.apache.maven.plugin.*,\
	!org.apache.tools.ant.*,\
	\
	*
Main-Class: com.liferay.tld.formatter.TLDFormatter
-includeresource:\
	@com.liferay.petra.xml-*.jar!/com/liferay/petra/xml/Dom4jUtil.class,\
	@com.liferay.portal.impl-*.jar!/com/liferay/portal/tools/ArgumentsMap.class,\
	@com.liferay.portal.impl-*.jar!/com/liferay/portal/tools/ArgumentsUtil.class,\
	@com.liferay.portal.impl-*.jar!/com/liferay/portal/util/EntityResolver.class,\
	@com.liferay.portal.impl-*.jar!/com/liferay/portal/xml/SAXReaderFactory.class,\
	@com.liferay.portal.kernel-*.jar!/com/liferay/portal/kernel/io/unsync/UnsyncBufferedReader.class,\
	@com.liferay.portal.kernel-*.jar!/com/liferay/portal/kernel/io/unsync/UnsyncByteArrayInputStream.class,\
	@com.liferay.portal.kernel-*.jar!/com/liferay/portal/kernel/io/unsync/UnsyncByteArrayOutputStream.class,\
	@com.liferay.portal.kernel-*.jar!/com/liferay/portal/kernel/io/unsync/UnsyncStringReader.class,\
	@com.liferay.portal.kernel-*.jar!/com/liferay/portal/kernel/log/*,\
	@com.liferay.portal.kernel-*.jar!/com/liferay/portal/kernel/search/Document.class,\
	@com.liferay.portal.kernel-*.jar!/com/liferay/portal/kernel/search/Field.class,\
	@com.liferay.portal.kernel-*.jar!/com/liferay/portal/kernel/search/highlight/HighlightUtil.class,\
	@com.liferay.portal.kernel-*.jar!/com/liferay/portal/kernel/security/RandomUtil.class,\
	@com.liferay.portal.kernel-*.jar!/com/liferay/portal/kernel/security/xml/SecureXMLFactoryProvider.class,\
	@com.liferay.portal.kernel-*.jar!/com/liferay/portal/kernel/security/xml/SecureXMLFactoryProviderUtil.class,\
	@com.liferay.portal.kernel-*.jar!/com/liferay/portal/kernel/util/*,\
	@com.liferay.portal.kernel-*.jar!/com/liferay/portal/kernel/xml/XMLSchema.class,\
	@xercesImpl-[0-9.]*.jar!/org/apache/xerces/xni/XNIException.class