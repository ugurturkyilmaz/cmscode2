package com.liferay.source.formatter.dependencies;

/**
 * @author Hugo Huijser
 */
public class ConstructorParameterOrder {

	public ConstructorParameterOrder(String value, String attribute) {
		_attribute = attribute;
		_name = "Hello World";
		_type = -1;
		_value = value;
	}

	public ConstructorParameterOrder(String value, String attribute, int type) {
		_attribute = attribute;
		_type = type;
		_value = value;
	}

	public ConstructorParameterOrder(
		String stringWithARatherUnnecessaryLongName1,
		String stringWithARatherUnnecessaryLongName2,
		String stringWithARatherUnnecessaryLongName3) {

		_stringWithARatherUnnecessaryLongName1 =
			stringWithARatherUnnecessaryLongName1;
		_stringWithARatherUnnecessaryLongName2 =
			stringWithARatherUnnecessaryLongName2;

		_stringWithARatherUnnecessaryLongName3 =
			stringWithARatherUnnecessaryLongName3;
	}

	private String _attribute;
	private String _name;
	private String _stringWithARatherUnnecessaryLongName1;
	private String _stringWithARatherUnnecessaryLongName2;
	private String _stringWithARatherUnnecessaryLongName3;
	private int _type;
	private String _value;

}