{{- if $.Values.slave.ports.vnc.enabled }}

{{- if $.Values.master.ports.vnc.enabled }}
kind: Ingress

apiVersion: extensions/v1beta1
metadata:
    name: {{ $masterPodName }}-ingress-resource
    namespace: {{ $namespace }}

spec:
    rules:
        - host: {{ $.Values.ingressHostname }}
          http:

              paths:
                  - backend:
                        serviceName: {{ $masterSubdomain }}
                        servicePort: 8080
                    path: /{{ $masterPodName }}
{{- end }}

{{- end }}