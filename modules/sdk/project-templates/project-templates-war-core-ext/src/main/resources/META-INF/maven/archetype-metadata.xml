<?xml version="1.0"?>

<archetype-descriptor name="liferay-module-war-core-ext">
	<fileSets>
		<fileSet filtered="true">
			<directory>src</directory>
			<includes>
				<include>**/*</include>
			</includes>
		</fileSet>
		<fileSet filtered="true">
			<directory></directory>
			<includes>
				<include>.gitignore</include>
				<include>build.gradle</include>
			</includes>
		</fileSet>
	</fileSets>
	<requiredProperties>
		<requiredProperty key="author">
		</requiredProperty>
		<requiredProperty key="liferayVersion">
			<validationRegex>^[0-7]\.\d+\.\d+.*</validationRegex>
		</requiredProperty>
	</requiredProperties>
</archetype-descriptor>