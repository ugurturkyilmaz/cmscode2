{
	"description_i18n": {
		"en_US": "Paste any Elasticsearch DSL query body into the element as-is."
	},
	"elementDefinition": {
		"category": "custom",
		"configuration": {
			"queryConfiguration": {
				"queryEntries": [
					{
						"clauses": [
							{
								"context": "query",
								"occur": "${configuration.occur}",
								"query": "${configuration.query}"
							}
						]
					}
				]
			}
		},
		"icon": "custom-field",
		"uiConfiguration": {
			"fieldSets": [
				{
					"fields": [
						{
							"defaultValue": "must",
							"label": "Occur",
							"name": "occur",
							"type": "select",
							"typeOptions": {
								"options": [
									{
										"label": "MUST",
										"value": "must"
									},
									{
										"label": "SHOULD",
										"value": "should"
									},
									{
										"label": "MUST NOT",
										"value": "must_not"
									},
									{
										"label": "FILTER",
										"value": "filter"
									}
								]
							}
						},
						{
							"defaultValue": {
							},
							"label": "Query",
							"name": "query",
							"type": "json"
						}
					]
				}
			]
		}
	},
	"title_i18n": {
		"en_US": "Paste Any Elasticsearch Query"
	}
}