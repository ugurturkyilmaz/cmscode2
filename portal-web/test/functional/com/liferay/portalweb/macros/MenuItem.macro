definition {

	macro click {
		Click(
			key_menuItem = "${menuItem}",
			locator1 = "MenuItem#ANY_MENU_ITEM");
	}

	macro clickNoError {
		ClickNoError(
			key_menuItem = "${menuItem}",
			locator1 = "MenuItem#ANY_MENU_ITEM");
	}

	macro viewDisabled {
		AssertElementPresent(
			key_menuItem = "${menuItem}",
			locator1 = "MenuItem#DISABLED_MENU_ITEM");
	}

	macro viewNotPresent {
		AssertElementNotPresent(
			key_menuItem = "${menuItem}",
			locator1 = "MenuItem#ANY_MENU_ITEM");
	}

	macro viewNotVisible {
		AssertNotVisible(
			key_menuItem = "${menuItem}",
			locator1 = "MenuItem#ANY_MENU_ITEM");
	}

	macro viewOrder {
		var key_index = "1";

		for (var menuItem : list "${menuItemList}") {
			if ("${menuItem}" != "-") {
				AssertTextEquals(
					index = "${key_index}",
					locator1 = "MenuItem#DROPDOWN_MENU_ITEM_SPECIFIC",
					value1 = "${menuItem}");
			}
			else {
				AssertElementPresent(
					index = "${key_index}",
					locator1 = "MenuItem#DROPDOWN_DIVIDER");
			}

			var key_index = ${key_index} + 1;
		}
	}

	macro viewPresent {
		AssertElementPresent(
			key_menuItem = "${menuItem}",
			locator1 = "MenuItem#ANY_MENU_ITEM");
	}

	macro viewVisible {
		AssertVisible(
			key_menuItem = "${menuItem}",
			locator1 = "MenuItem#ANY_MENU_ITEM");
	}

}