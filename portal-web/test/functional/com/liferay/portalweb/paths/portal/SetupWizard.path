<html>
<head>
<title>SetupWizard</title>
</head>

<body>
<table border="1" cellpadding="1" cellspacing="1">
<thead>
<tr><td rowspan="1" colspan="3">SetupWizard</td></tr>
</thead>

<tbody>
<tr>
	<td>BASIC_CONFIGURATION</td>
	<td>//div[@id='main-content']/form</td>
	<td></td>
</tr>
<tr>
	<td>CONFIGURATION_SAVE_MESSAGE</td>
	<td>//p[@class='lfr-setup-notice']</td>
	<td></td>
</tr>
<tr>
	<td>CONFIGURATION_SAVE_LOCATION</td>
	<td>//span[@class='lfr-inline-code']</td>
	<td></td>
</tr>
<tr>
	<td>SETUP_WIZARD_BASIC_CONFIGURATION_PNG</td>
	<td>portal/screenshots/setupwizard/setup_wizard_basic_configuration.png</td>
	<td></td>
</tr>
<tr>
	<td></td>
	<td></td>
	<td></td>
</tr>
<!--DEFAULT_LANGUAGE-->
<tr>
	<td>DEFAULT_LANGUAGE</td>
	<td>//div[contains(.,'Default Language')]//select</td>
	<td></td>
</tr>
<!--DATABASE-->
<tr>
	<td>DATABASE_CONFIGURED_JDBC_DRIVER_CLASS_NAME_LABEL</td>
	<td>//div[@id='defaultDatabaseOptions']/*[normalize-space(text())='Configured Database']</td>
	<td></td>
</tr>
<tr>
	<td>DATABASE_DEFAULT_LABEL</td>
	<td>//div[@id='defaultDatabaseOptions']/p/strong[contains(text(),'Default Database (Hypersonic)')]</td>
	<td></td>
</tr>
<tr>
	<td>DATABASE_DEFAULT_DESCRIPTION</td>
	<td>//div[@id='defaultDatabaseOptions']</td>
	<td></td>
</tr>
<tr>
	<td>DATABASE_CHANGE_LINK</td>
	<td>//a[@id='customDatabaseOptionsLink']</td>
	<td></td>
</tr>
<tr>
	<td></td>
	<td></td>
	<td></td>
</tr>
<!--DATABASE_CHANGE-->
<tr>
	<td>DATABASE_CHANGE_USE_CONFIGURED_DATABASE_LINK</td>
	<td>//a[@id='defaultDatabaseOptionsLink']</td>
	<td></td>
</tr>
<tr>
	<td>DATABASE_CHANGE_DATABASE_TYPE_LABEL</td>
	<td>//label[@for='databaseType']</td>
	<td></td>
</tr>
<tr>
	<td>DATABASE_CHANGE_DATABASE_TYPE_SELECT</td>
	<td>//select[@id='databaseType']</td>
	<td></td>
</tr>
<tr>
	<td>DATABASE_CHANGE_JDBC_URL_LABEL</td>
	<td>//label[@for='jdbcDefaultURL']</td>
	<td></td>
</tr>
<tr>
	<td>DATABASE_CHANGE_JDBC_URL_FIELD</td>
	<td>//input[@id='jdbcDefaultURL']</td>
	<td></td>
</tr>
<tr>
	<td>DATABASE_CHANGE_JDBC_DRIVER_CLASS_NAME_LABEL</td>
	<td>//label[@for='jdbcDefaultDriverName']</td>
	<td></td>
</tr>
<tr>
	<td>DATABASE_CHANGE_JDBC_DRIVER_CLASS_NAME_FIELD</td>
	<td>//input[@id='jdbcDefaultDriverName']</td>
	<td></td>
</tr>
<tr>
	<td>DATABASE_CHANGE_USER_NAME_LABEL</td>
	<td>//label[@for='jdbcDefaultUserName']</td>
	<td></td>
</tr>
<tr>
	<td>DATABASE_CHANGE_USER_NAME_FIELD</td>
	<td>//input[@id='jdbcDefaultUserName']</td>
	<td></td>
</tr>
<tr>
	<td>DATABASE_CHANGE_PASSWORD_LABEL</td>
	<td>//label[@for='jdbcDefaultPassword']</td>
	<td></td>
</tr>
<tr>
	<td>DATABASE_CHANGE_PASSWORD_FIELD</td>
	<td>//input[@id='jdbcDefaultPassword']</td>
	<td></td>
</tr>
</tbody>
</table>
</body>
</html>