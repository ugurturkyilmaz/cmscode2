<html>
<head>
<title>UserBar</title>
</head>

<body>
<table border="1" cellpadding="1" cellspacing="1">
<thead>
<tr><td rowspan="1" colspan="3">UserBar</td></tr>
</thead>

<tbody>
<tr>
	<td>USER_AVATAR_DROPDOWN_ITEM</td>
	<td>//div[contains(@class,'dropdown-menu-personal-menu') and contains(@class,'show')]//a[contains(.,'${key_dropdownItem}')]</td>
	<td></td>
</tr>
<tr>
	<td>USER_AVATAR_DROPDOWN_PORTAL_CLOSED</td>
	<td>//div[contains(@class,'dropdown-menu-personal-menu')]</td>
	<td></td>
</tr>
<tr>
	<td>USER_AVATAR_DROPDOWN_PORTAL_OPEN</td>
	<td>//*[contains(@class,'dropdown-menu') and contains(@class,'show')]</td>
	<td></td>
</tr>
<tr>
	<td>USER_AVATAR_DROPDOWN_MYRAYLIFE</td>
	<td>//a[contains(@class,'dropdown-item')][normalize-space(text())='My Raylife']</td>
	<td></td>
</tr>
<tr>
	<td>USER_AVATAR_DROPDOWN_SIGNOUT</td>
	<td>//a[contains(@class,'dropdown-item')][contains(.,'Sign Out') or contains(.,'Salir')]</td>
	<td></td>
</tr>
<tr>
	<td>USER_AVATAR_ICON</td>
	<td>//span[@class='user-avatar-link']//span[*[name()='svg'][contains(@class,'lexicon-icon-user')]]</td>
	<td></td>
</tr>
<tr>
	<td>USER_AVATAR_BUTTON</td>
	<td>//button[@aria-label='Personal Menu']</td>
	<td></td>
</tr>
<tr>
	<td>USER_AVATAR_ICON_PNG</td>
	<td>portal/screenshots/user/user_avatar_icon.png</td>
	<td></td>
</tr>
<tr>
	<td>USER_AVATAR_IMAGE</td>
	<td>//span[@class='user-avatar-link']</td>
	<td></td>
</tr>
<tr>
	<td>USER_AVATAR_IMAGE_LOADED</td>
	<td>//span[@class='sticker-overlay']//img[@class = 'img-fluid']</td>
	<td></td>
</tr>
<tr>
	<td>USER_AVATAR_IN_CONTROL_MENU</td>
	<td>//div[contains(@class,'control-menu')]//div[contains(@class,'personal-menu-dropdown')]</td>
	<td></td>
</tr>
<tr>
	<td>USER_AVATAR_IN_NAVIGATION_USER_PERSONAL_BAR_PORTLET</td>
	<td>//div[contains(@class,'portlet-user-personal-bar')]//div[contains(@class,'personal-menu-dropdown')]</td>
	<td></td>
</tr>
<tr>
	<td>USER_AVATAR_PORTRAIT</td>
	<td>//button//img[contains(@src,'user_portrait')]</td>
	<td></td>
</tr>
<tr>
	<td>USER_AVATAR_TOGGLE</td>
	<td>//button[contains(@data-toggle,'dropdown')]//span[contains(@class,'user-icon')]</td>
	<td></td>
</tr>
<tr>
	<td>USER_DASHBOARD</td>
	<td>//a[contains(.,'Dashboard')]</td>
	<td></td>
</tr>
<tr>
	<td>USER_FULL_NAME</td>
	<td>//li[@class='dropdown-subheader']</td>
	<td></td>
</tr>
<tr>
	<td>USER_ORGANIZATIONS</td>
	<td>//a[contains(.,'Organizations')]</td>
	<td></td>
</tr>
<tr>
	<td>USER_PROFILE</td>
	<td>//a[contains(.,'Profile')]</td>
	<td></td>
</tr>
<tr>
	<td>USER_SIGN_OUT</td>
	<td>//a[contains(.,'Sign Out')] | //a[@href='/c/portal/logout']</td>
	<td></td>
</tr>
<tr>
	<td>USER_SIGN_IN</td>
	<td>//span[contains(@class,'sign-in')]/a/span</td>
	<td></td>
</tr>
<tr>
	<td>USER_SIGN_IN_PNG</td>
	<td>portal/screenshots/user/user_personal_bar_sign_in.png</td>
	<td></td>
</tr>
</tbody>
</table>
</body>
</html>